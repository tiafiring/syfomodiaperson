    <script>
        var finnMiljoStreng = function() {
            var host = window.location.host;
            var bindestrekIndex = host.indexOf('-');
            if (bindestrekIndex === -1) {
                return '';
            }
            var dotIndex = host.indexOf('.');
            return host.substring(bindestrekIndex, dotIndex);
        };

        var script = document.createElement('script');
        script.src = 'https://app' + finnMiljoStreng() + '.adeo.no/internarbeidsflatedecorator/js/head.min.js?rev=' + Date.now().toString();
        script.onload = function() {
            var DOMContentLoaded_event = document.createEvent("Event");
            DOMContentLoaded_event.initEvent("DOMContentLoaded", true, true);
            window.document.dispatchEvent(DOMContentLoaded_event);
        };
        document.head.appendChild(script);
    </script>
</head>
