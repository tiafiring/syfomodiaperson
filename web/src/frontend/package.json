{
  "name": "modiasyfofront",
  "version": "0.0.1",
  "description": "Digitalisering av sykefraværsoppfølging",
  "main": "index.js",
  "test": "npm run test",
  "scripts": {
    "prebuild": "node createProdFolders.js",
    "dev": "title modiasyfofront && npm-run-all prebuild images static:build:local scripts:watch",
    "build": "npm-run-all prebuild images static:build scripts:build",
    "test:watch": "title modiasyfofront && cross-env NODE_ENV=test nyc mocha --watch -G --require ignore-styles --compilers js:babel-core/register --require babel-polyfill ./test/setup.js --recursive test",
    "test": "cross-env NODE_ENV=test nyc mocha -G --require ignore-styles --compilers js:babel-core/register --require babel-polyfill ./test/setup.js --recursive test",
    "posttest": "nyc report --reporter=html --reporter=lcov",
    "static:build:local": "title modiasyfofront && node build-html.js html/modiasyfofront.mustache local",
    "static:build": "node build-html.js html/modiasyfofront.mustache prod",
    "scripts:build": "set NODE_ENV=production&&node deploy.js",
    "scripts:watch": "title modiasyfofront && webpack-dev-server --inline  --port 3040",
    "lint": "eslint js/. --ext .js",
    "lint:fix": "eslint js/. --ext .js --fix",
    "images": "cp -R img ../main/webapp"
  },
  "nyc": {
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "author": "Team DigiSyfo <digisyfo@nav.no>",
  "license": "ISC",
